{
  "name": "template-nextjs-tailwind",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "start": "yarn dev",
    "dev": "concurrently \"yarn:dev:*\"",
    "dev:reflect": "npx reflect dev",
    "dev:next": "next dev",
    "dev:tailwind": "npx tailwindcss -i src/styles/style.css -o src/styles/build.css --minify --watch",
    "build": "yarn build:tailwind && yarn build:next",
    "build:next": "next build",
    "build:tailwind": "npx tailwindcss -i src/styles/style.css -o src/styles/build.css --minify",
    "update:all": "yarn upgrade-interactive",
    "generate": "yarn prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@rocicorp/reflect": "^0.39.202401100534",
    "next": "14.0.3",
    "prisma": "^5.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.3.5"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "18.18.10",
    "@types/react": "18.2.37",
    "concurrently": "^8.2.2",
    "postcss": "8.4.31",
    "typescript": "5.2.2"
  },
  "packageManager": "yarn@4.0.1",
  "engines": {
    "node": ">=18.17.0"
  }
}
